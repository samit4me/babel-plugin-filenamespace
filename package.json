{
  "name": "babel-plugin-filenamespace",
  "version": "1.0.1",
  "description": "Babel plugin to generate a namespace based on filename",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --ignore spec.js,test.js",
    "clean": "rimraf lib coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run clean && npm run build",
    "lint": "eslint . --ignore-path .gitignore --ignore-pattern testFixtures",
    "test": "npm run prepublish && npm run lint && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samit4me/babel-plugin-filenamespace.git"
  },
  "keywords": [
    "namespace",
    "namespacing",
    "redux",
    "action",
    "type",
    "module",
    "babel",
    "babel-plugin",
    "filename",
    "dirname"
  ],
  "author": "Samuel Sharpe",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-jest": "15.0.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "coveralls": "2.11.14",
    "cross-env": "3.1.3",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "jest-cli": "16.0.2",
    "rimraf": "2.5.4"
  }
}
