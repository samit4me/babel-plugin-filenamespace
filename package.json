{
  "name": "babel-plugin-filenamespace",
  "version": "0.0.5",
  "description": "Babel plugin to generate a namespace based on filename",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run prepublish && jest",
    "clean": "rimraf lib coverage",
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --ignore spec.js,test.js",
    "prepublish": "npm run clean && npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samit4me/babel-plugin-filenamespace.git"
  },
  "author": "Samuel Sharpe",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.14.0",
    "coveralls": "^2.11.14",
    "cross-env": "^2.0.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "jest-cli": "^15.1.1",
    "rimraf": "^2.5.4"
  }
}
